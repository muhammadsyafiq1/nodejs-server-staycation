<div class="tab-pane fade show active" id="image" role="tabpanel" aria-labelledby="image-tab">
    <div class="row">
        <% for( let x = 0; x < item.imageId.length; x ++ ) { %>
            <div class="col-md-4">
                <div class="card shadow mb-4 mt-2">
                    <div class="card-body">
                        <img src="http://localhost:3300/<%= item.imageId[x].imageUrl %>" alt="imageUrl" style="width: 100%; display: block;">
                    </div>
                </div>
            </div>
        <% } %>
    </div>
    <a href="/admin/item" class="btn btn-sm btn-warning" >
        <i class="fa fa-arrow-left"></i>
    </a>
</div>